{
    "name": "algolia/algoliasearch-laravel-4",
    "description": "Laravel 4 Algolia extension",
    "license": "MIT",
    "keywords": [
        "laravel",
        "algolia",
        "search",
        "api"
    ],
    "require": {
    	"php": ">=5.3.29",
        "algolia/algoliasearch-client-php": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "orchestra/testbench": "2.2.*"
    },
    "autoload": {
        "psr-4": {
            "AlgoliaSearch\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlgoliaSearch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
